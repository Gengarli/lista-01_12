import java.util.Scanner;

class Main {
  
  public static void main(String[] args) {
     
     Scanner teclado = new Scanner(System.in);
     
     System.out.print("Qual é a sua altura? : ");
     double a = teclado.nextDouble();

     double p = 72.7 * a - 58;
    
     System.out.println("O seu peso ideal é: " + p + "!");

     }

}
